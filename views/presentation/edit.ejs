<script type="text/javascript">	
	var options = <%-options%>; 
	<% var mypres = JSON.parse(presentation.content);
	if(mypres.id ===""){
		mypres.id = presentation._id;
	}%>
	var presentation = <%-JSON.stringify(mypres)%>;
</script>

<h2> Title <%=presentation.title%> </h2>
<p> Description <%=presentation.description%> </p>
<p> Author <%=author%> </p>
<% if (presentation.avatar) {%>
	<p><img src='<%=presentation.avatar%>'></p>
<%}%>
<p><a href='/presentation/<%=presentation._id%>'>Show</a></p>
<div style='width:900px;height:750px;margin:0 auto; display:block'>
	<iframe src='/vishEditor/edit.html' style='border:0' width='910' height='750' iframeborder='0' frameborder='0' iframeElement.frameBorder = 0 id="presentation_iframe"/>
</div>
